	#nav #nav-search-form:hover {
		background-color: hsla(0, 0%, 100%, .4)
	}
}

#nav #nav-search-form:active,
#nav #nav-search-form:focus,
#nav #nav-search-form:focus-within {
	background-color: #fff
}

#nav #nav-search-form:active #nav-mobile-search-input,
#nav #nav-search-form:active #nav-search-input,
#nav #nav-search-form:focus #nav-mobile-search-input,
#nav #nav-search-form:focus #nav-search-input,
#nav #nav-search-form:focus-within #nav-mobile-search-input,
#nav #nav-search-form:focus-within #nav-search-input {
	font-size: 14px!important;
	font-style: normal!important;
	font-weight: 300!important;
	line-height: 150%!important;
	letter-spacing: normal!important;
	color: #403f5d
}

